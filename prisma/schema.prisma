generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Mineral {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  description String?
  tags        String?    
  specs       String?   
  model       String?
  img         String?
}